{% extends 'template.html.twig' %}
{% block body %}
<div>
    <ol class="list-group">
        {% for name,content in app.session.get('todos') %}
            <li class="list-group-item">{{ name | upper}} : {{ content | capitalize}}</li>
        {% endfor %}
    </ol>
</div>
{% endblock %}
{% block bodyTitle %}Liste des todos{% endblock %}
{% block navTitle%}CV Tech{% endblock %}